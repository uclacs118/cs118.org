---
import type { GetStaticPaths } from "astro";

export const links: any = {
    'form': 'http://google.com',
    'laform': 'https://ucla.edu',
    'zoom': 'https://ucla.zoom.us/j/97726470833?pwd=G1qgVbT89oVLIp2MVXYeFYaX47TgOY.1',
    '1a': 'https://ucla.zoom.us/j/93017095175?pwd=HsNASbPTaoUZ1paEunBOzDtWJsO8O0.1',
    '1b': 'https://ucla.zoom.us/j/99248480180?pwd=7q9JuqBvXLCLY9d37R1bsu04qSbMgh.1',
    '1c': 'https://ucla.zoom.us/j/96796668770?pwd=e9rdCmrbkXmLebgUJUDwrbEKoeW5WD.1'
}

export const getStaticPaths = (() => {
    return Object.keys(links).map(link => { return {params: {auto: link}}});
}) satisfies GetStaticPaths;

const { auto } = Astro.params;
---

<!doctype html>
<html lang="en-US">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="refresh" content=`0; url=${links[auto]}` />
        <script is:inline type="text/javascript">
            window.location.href = links[auto];
        </script>
        <title>Page Redirection</title>
    </head>
    <body>
        <!-- Note: don't tell people to `click` the link, just tell them that it is a link. -->
        If you are not redirected automatically, follow this <a
            href={links[auto]}>link to example</a
        >.
    </body>
</html>
